ClickEffect: {
	#Pre-Hardmode
	BasicAutoclicker: {
		Name: Basic Autoclicker
		Description: Allow auto-clicking with 7.5 clicks per second. Right Click to toggle this on or off
	}

	Sandstorm: {
		Name: Sandstorm
		Description: Creates a sandstorm explosion
	}

	SeaBubble: {
		Name: Sea Bubble
		Description: Creates a slowly damaging bubble that floats away
	}

	Mushy: {
		Name: Mushy
		Description: Grants the Mushy buff, which increases defense and life regen
	}

	SulphurousBubbles: {
		Name: Sulphurous Bubbles
		Description: Creates 4-5 bubbles that can be clicked on to create a damaging area that inflicts Irradiated
	}

	BloodyExplosion: {
		Name: Bloody Splash
		Description: Creates a splash explosion that inflicts Burning Blood
	}

	BrainExplosion: {
		Name: Brain Explosion
		Description: Creates an explosion of rotten brain matter that inflicts Brain Rot
	}

	FeatherRain: {
		Name: Feather Rain
		Description: Rains down explosive feathers
	}

	ReelIn: {
		Name: Reel In
		Description: Summons a clickerfin. Killing it gives increases click damage by 25% for 10 seconds
	}

	SlimePuppet: {
		Name: Slime Puppet
		Description: Creates a slime ball which will make a short dash towards the cursor after a short period of time
	}

	# Hardmode
	GaussFlux: {
		Name: Gauss Flux
		Description: Creates a turbulent magnetic force
	}

	BrimstoneInferno: {
		Name: Brimstone Inferno
		Description: Creates an explosion in the form of a satanic sigil that inflicts Brimstone Flames
	}

	CryoBomb: {
		Name: Cryo Bomb
		Description: Creates a cryobomb near the cursor that can be clicked on to release icy shards
	}

	DaedalusCrystals: {
		Name: Daedalus Crystals
		Description: Creates several homing daedalus bolts
	}

	Crunch: {
		Name: Crunch
		Description: Inflicts Armor Crunch for 3 seconds on the nearest enemies
	}

	ChaosFlame: {
		Name: Chaos Flame
		Description: Create small portal-like flame that inflicts Brimstone Flames
	}

	#Post-Plantera
	TerraAura: {
		Name: Terra Aura
		Description: Creates a edenic damaging aura that lasts 3 seconds
	}

	GreenInfection: {
		Name: Green Infection
		Description: Creates {0} burning green ichor streams that home in on enemies
	}

	AnahitasVoice: {
		Name: Anahita's Voice
		Description: Creates a circle of damaging musical notes
	}

	CoolFlake: {
		Name: Cool Flake
		Description: Creates a snowflake minion. If there is already a snowflake minion, refresh its lifetime
	}

	AureusBlast: {
		Name: Auroradical Blast
		Description: Shoots laser-fast aureus blasts for every nearby enemy around the cursor's position
	}

	StellarScythe: {
		Name: Stellar Scythe
		Description: Creates a piercing scythe that flies towards the cursor after a short period of time
	}

	# Post-Golem
	PlagueBees: {
		Name: Plague Bees
		Description: Summons a lot of plague seekers that inflict the Plague
	}

	Eruption: {
		Name: Eruption
		Description: Creates a large scorching explosion
	}

	StarRain: {
		Name: Star Rain
		Description: Rains down piercing astral stars
	}

	Stardust: {
		Name: Stardust
		Description: Creates a stardust explosion
	}

	Entropy: {
		Name: Head Death
		Description: Creates a large meld energy explosion
	}

	BloodBlasts: {
		Name: Blood Blasts
		Description: Shoots 8 blood shots diagonally towards the cursor
	}

	TaurusFang: {
		Name: Taurus' Fang
		Description: Creates 13 homing Arid Fangs that impale enemies
	}

	# Post-Moonlord
	# Early Stage
	
	ElementalAura: {
		Name: Elemental Aura
		Description: Creates a damaging elemental aura that lasts 2 seconds and inflicts Elemental Mix
	}

	PlagueBees2: {
		Name: "{$Mods.CalamityClickers.ClickEffect.PlagueBees.Name}"
		Description: "{$Mods.CalamityClickers.ClickEffect.PlagueBees.Description}"
	}

	RedLightning: {
		Name: Red Lightning
		Description: Causes 4 lightning bolts to rain down from the sky
	}

	#Post-Providence
	ProfanedInferno: {
		Name: Profaned Inferno
		Description: Creates a holy flare explosion that inflicts Holy Flames for 3 seconds
	}

	TarragonThorns: {
		Name: Tarragon Thorns
		Description: Creates 8 thorns which grow outward
	}

	Judgement: {
		Name: Judgement
		Description: Calls forth a righteous column of light that smites and delivers judgement upon those who are foolish enough to oppose you
	}

	NovaSlimer: {
		Name: Nova Slimer
		Description: Summons a homing Novaslime
	}

	# Post-Polterghast
	GhastlyPortal: {
		Name: Call to the Beyond
		Description: Summons a Ghastly Portal which spews forth homing souls in response to your clicks
	}

	StratusMoon: {
		Name: Stratus Moon
		Description: Creates a circle of moons that fly towards your cursor after a while
	}

	Lifestealing: {
		Name: Lifestealing
		Description: Heals you by 2 hp if the target is Marked for Death
	}

	BloodyKnives: {
		Name: Bloody Knives
		Description: Creates knives that impale enemies and inflict Burning Blood and Marked for Death
	}

	PhantasmalReach: {
		Name: Phantasmal Reach
		Description: Deals damage in accordance with the distance from the player; can go as low as 0.75x and up to 1.75x damage at max range
	}

	# Post-DoG
	NebulaStars: {
		Name: Nebula Stars
		Description: Summons a circle of stars that home in on enemies after a short period
	}

	Distort: {
		Name: Distort
		Description: Summons a flurry of piercing knives that move in reaction to your clicks
	}

	NightmareMagic: {
		Name: Nightmare Magic
		Description: Randomly acts as any possible click effect
	}

	Totality2: {
		Name: Super Totality
		Description: Creates a miniature darksun that repeatedly fires various homing projectiles
	}

	NuclearExplosion: {
		Name: Nuclear Explosion
		Description: BOOM!
	}

	# Post-Yharon
	PhoenixWrath: {
		Name: Phoenix Wrath
		Description: Calls in dragons to scorch the area with Dragonfire inflicting flames
	}

	MiceCatalyst: {
		Name: Mice Catalyst
		Description: Summons a catalyst cloud that creates homing nebula projectiles
	}

	Popstar: {
		Name: Popstar
		Description: Drops a large omega star that splits into three rings of smaller stars that start homing after a while
	}

	# Pre-Endgame Bosses
	MiniExoTwins: {
		Name: Olympic Call
		Description: Summons mini Artemis and Mini Apollo to attack the cursor
	}

	Exomination: {
		Name: Exomination
		Description: Creates a large exo energy explosion
	}

	LecherousOrb: {
		Name: Lecherous Orb
		Description: Creates Lecherous Orbs that accumulate life hearts and can be clicked to pop them open. This effect costs 20 hp
	}

	# Endgame
	ItzClick: {
		Name: Itz Click
		Description: Inflicts a very powerful damage over time debuff
	}

	Mischief: {
		Name: Mischief
		Description: Summons a mini rat clicker that can trigger clicker effects of nearby clickers in the inventory (except Mischief)
	}

	# Equip
	DaedalusLightning: {
		Name: Daedalus Lightning
		Description: Attracts the power of Daedalus from the sky
	}

	BurnOrBliss: {
		Name: Burn or Bliss
		Description: Releases a burst of damaging normal, spicy and white chocolates
	}

	# Draedon
	ScrapMissile: {
		Name: Scrap Missile
		Description: Shoots a missle from above towards the target that shatters on impact
	}

	DrillShot: {
		Name: Drill Shot
		Description: Shoots a Drill that latches onto enemies.
	}

	Electrofield: {
		Name: Electrofield
		Description: Summons a lingering electromagnetic field that extends on hit.
	}

	VitalInfusion: {
		Name: Vital Infusion
		Description: Charges a green BioNeedle next to player. Right Click to heal 25 health at the cost of one needle.
	}

	CurrentStrike: {
		Name: Current Strike
		Description: Fires a powerful current of ionized particles that arcs through enemies.
	}
}

